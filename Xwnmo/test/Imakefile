XCOMM ifndef uniosu
XCOMM ifndef	HasGcc
XCOMM define	HasGcc	YES
XCOMM endif
XCOMM endif

ARCS =  $(XMULIB) $(XLIB)

LOCAL_LIBRARIES = $(ARCS)

#ifdef uniosu
SYSLIBS= -lcurses -lj
#else
SYSLIBS= -lcurses -ltermcap
#endif

INCLUDES = -I$(TOP) -I$(XLIBSRC)

TOP_INCLUDES =

OBJS1 =	ximtest.o \
	curses.o \
	xresource.o \
	chkim.o	\
	chkic.o	\
	chkutil.o \
	keyev.o	\
	chkfe.o	\
	chkxls.o \
	chkcset.o \
	chkpsset.o \
	callback.o \
	chkreset.o \
	funcv.o	\
	final.o

SRCS1 =	ximtest.c \
	curses.c \
	xresource.c \
	chkim.c	\
	chkic.c	\
	chkutil.c \
	keyev.c	\
	chkfe.c	\
	chkxls.c \
	chkcset.c \
	chkpsset.c \
	callback.c \
	chkreset.c \
	funcv.c	\
	final.c


AllTarget(ximtest)
SingleProgramTarget(ximtest, $(OBJS1), $(LOCAL_LIBRARIES), $(SYSLIBS))




