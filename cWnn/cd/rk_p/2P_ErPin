(defvar A  	(list B C I D F G H   K L M N P     S U T W   Y Z V))
(defvar AI  	(list B C I D   G H   K L M N P     S U T W     Z V))
(defvar AN  	(list B C I D F G H   K L M N P   R S U T W   Y Z V)) ;ANG
(defvar AO  	(list B C I D   G H   K L M N P   R S U T     Y Z V))
(defvar E  	(list   C I D   G H   K L M N     R S U T     Y Z V))
(defvar EI  	(list B C   D F G H   K L M N P       U T W     Z V))
(defvar EN  	(list B C I D F G H   K   M N P   R S U   W     Z V))
(defvar ENG  	(list B C I D F G H   K L M N P   R S U T W     Z V))
(defvar I  	(list B C I D       J   L M N P Q R S U T   X Y Z V))
(defvar IA  	(list       D       J   L       Q           X      )) ;nia;
(defvar IAN  	(list B     D       J   L M N P Q       T   X      ))
(defvar IANG  	(list               J   L   N   Q           X      ))
(defvar IAO	(list B     D       J   L M N P Q       T   X      )) ;IE
(defvar IN  	(list B             J   L M N P Q           X Y    ))
(defvar ING  	(list B     D       J   L M N P Q       T   X Y    ))
(defvar IONG  	(list               J           Q           X      ))
(defvar IU	(list       D       J   L M N   Q           X      ))
(defvar O  	(list B       F           M   P           W   Y    )) ;lo;
(defvar ONG  	(list   C I D   G H   K L   N     R S   T     Y Z V))
(defvar OU	(list   C I D F G H   K L M N P   R S U T     Y Z V))
(defvar U  	(list B C I D F G H J K L M N P Q R S U T W X Y Z V))
(defvar V  	(list                   L   N                      ))
(defvar UA  	(list     I     G H   K           R   U           V))
(defvar UAI  	(list     I     G H   K               U           V))
(defvar UAN	(list   C I D   G H J K L   N   Q R S U T  X Y Z  V))
(defvar UANG  	(list     I     G H   K               U           V))
(defvar UE  	(list               J           Q           X Y    ))
(defvar VE  	(list                   L   N                      ))
(defvar UI  	(list   C I D   G H   K           R S U T       Z V))
(defvar UN  	(list   C I D   G H J K L       Q R S U T   X Y Z V))
(defvar UO  	(list   C I D   G H   K L   N     R S U T       Z V))
